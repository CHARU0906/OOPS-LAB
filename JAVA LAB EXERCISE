import java.util.*;
class Person{
String name;
int age;

Person(String name, int age){
this.name = name;
this.age = age;
}

void displayInfo(){
System.out.println(name);
System.out.println(age);
}
}

class p{
public static void main(String[] args){
Scanner n = new Scanner(System.in);
String name = n.nextLine();
int age = n.nextInt();
Person p = new Person(name, age);
p.displayInfo();
}
}
